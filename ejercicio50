program Ejercicio50;
    const 
        max= 50;
    type
        tcapacidad =0..max;  
        tNum1 = array [1..50] of tcapacidad;
    var 
        result, i, j: integer;
        suma: tnum1;

    begin
        randomize;
        for i := 1 to max do 
            j:= random(10);
            result:= suma[i]+j;
            writeln (result)
    end. 
